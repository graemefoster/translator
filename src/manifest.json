{
    "name": "GPTTranslator",
    "description": "",
    "version": "0.0.1",
    "manifest_version": 3,
    "permissions": [
        "storage"
    ],
    "action": {
        "default_title": "GPTTranslator 0.0.1"
    },
    "options_page": "options.html",
    "content_scripts": [
        {
            "matches": [
                "http://localhost/*",
                "https://*/*"
            ],
            "js": [
                "bundle.js"
            ],
            "run_at": "document_start"
        }
    ],
    "key": ""
}